= simple_form_for @competency do |f|
  - if @competency.errors.any?
    #error_explanation
      %h2= "#{pluralize(@competency.errors.count, "error")} prohibited this competency from being saved:"
      %ul
        - @competency.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :title
    = f.text_field :title
  .field
    = f.association :questions
  .field
    = f.association :videos

  .actions
    = f.submit 'Save'